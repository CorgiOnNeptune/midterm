{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "mocha": true,
    "jquery": true
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "formatDate": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-unused-vars": "warn",
    "object-shorthand": "warn",
    "prefer-template": "warn",
    "prefer-destructuring": "warn",
    "no-param-reassign": "warn"
  }
}
